# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# API generated by apidoc.me
GET     /page_info/info_for_url                      controllers.PageInfo.getInfoForUrl(url: String)
GET     /page_info/open_graph_metadata_for_url       controllers.PageInfo.getOpenGraphMetadataForUrl(url: String)
GET     /page_info/apple_itunes_metadata_for_url     controllers.PageInfo.getAppleItunesMetadataForUrl(url: String)
GET     /page_info/search_metadata_for_url           controllers.PageInfo.getSearchMetadataForUrl(url: String)
GET     /page_info/twitter_card_metadata_for_url     controllers.PageInfo.getTwitterCardMetadataForUrl(url: String)
GET     /page_info/schema_org_items_for_url          controllers.PageInfo.getSchemaOrgItemsForUrl(url: String)

# Home page
GET        /                             controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                 controllers.Assets.at(path="/public", file)
